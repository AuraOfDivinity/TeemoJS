{
  "name": "teemojs",
  "version": "2.0.0-alpha.5",
  "description": "A Fast & Lightweight Riot API Wrapper",
  "files": [
    "src/*",
    "dist/*",
    "config/*",
    "*.md"
  ],
  "main": "src/index.js",
  "browser": "dist/browser.js",
  "scripts": {
    "prepare": "node ./node_modules/browserify/bin/cmd.js src/index.js -o dist/browser.js -s TeemoJS --no-bf",
    "test": "node ./node_modules/mocha/bin/mocha",
    "build": "node gen && node ./node_modules/browserify/bin/cmd.js src/index.js -o dist/browser.js -s TeemoJS --no-bf"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MingweiSamuel/TeemoJS.git"
  },
  "keywords": [
    "riot",
    "games",
    "riot games",
    "api",
    "league",
    "legends",
    "league of legends",
    "wrapper",
    "teemo",
    "kindred"
  ],
  "author": "Mingwei Samuel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MingweiSamuel/TeemoJS/issues"
  },
  "homepage": "https://github.com/MingweiSamuel/TeemoJS#readme",
  "dependencies": {
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "browserify": "^16.5.0",
    "mocha": "^5.2.0",
    "mocha.parallel": "^0.15.6",
    "node-fetch": "^2.6.0"
  }
}
